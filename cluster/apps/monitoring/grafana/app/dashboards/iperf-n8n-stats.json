          "rawQuery": true,
          "rawSql": "SELECT\r\n  l.\"created_at\" AS time,\r\n  l.\"average_speed\" AS value,\r\n  COALESCE(sh.\"alias\", sh.\"host\") || ' -> ' || COALESCE(th.\"alias\", th.\"host\") AS metric\r\nFROM iperf_logs l\r\nJOIN iperf_hosts sh ON sh.id = l.source_host_id\r\nJOIN iperf_hosts th ON th.id = l.target_host_id\r\nWHERE\r\n  $__timeFilter(l.created_at)\r\n  AND (\r\n    -- host_filter: matches either source or target\r\n    CASE\r\n      WHEN '0' IN ($host_filter) THEN false\r\n      WHEN '0' IN ($second_host_filter) THEN false\r\n      ELSE true\r\n    END\r\n  )\r\n  AND (\r\n    (l.\"source_host_id\" IN ($host_filter) AND l.\"target_host_id\" IN ($second_host_filter))\r\n    OR\r\n    (l.\"target_host_id\" IN ($host_filter) AND l.\"source_host_id\" IN ($second_host_filter))\r\n  )\r\nORDER BY l.created_at",
